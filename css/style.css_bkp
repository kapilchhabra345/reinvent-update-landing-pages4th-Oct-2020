    padding: 6px 10px;
    border-radius: 5px;
    transition: 1s ease;
    border:2px solid #871511;
}

#patient_story_carousel .owl-prev:hover, #patient_story_carousel .owl-next:hover {
    background-color: transparent;
    color: #871511;
    transition: 1s ease;
}

.patient_story_sec{
	padding-bottom: 40px;
}


