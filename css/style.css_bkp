/*----  Common styles  ----*/

@font-face {
    font-family: PT Sans Regular;
    src: url('../fonts/PT Sans Regular.ttf') format('TrueType');
}

body{
  overflow-x:hidden;
  font-family: PT Sans Regular;  
}
h1, h2, h3, h4, h5, h6{
	font-family:PT Sans Regular;
}

.container{
  width:100%;
  max-width:1400px;
  margin:0 auto;
  padding:0 40px;	
}
section{
  width:100%;
}
.main_heading{
	width:100%;
	margin-bottom:30px;
}
.main_heading h2{
  font-size:46px;
  line-height:56px;
  color:#ec8226;
  font-weight:700;
  text-transform:uppercase;	
}
.w3-row{
	margin-left:-15px;
	margin-right:-15px;
}
.w3-col{
	padding:15px;
}
.tab_content{
	display:none;
}
.error{
   border-bottom-color: red!important;
   background-image: url(../images/exclamation.png) !important;
   background-position: 92%;
   background-repeat: no-repeat;
}
.w3-row.owl-carousel{
    margin:0;
}

/*----  Common styles end here ----*/


/*--  header start here  --*/
header{
  width:100%;
  padding:20px 0;
  position:absolute;
  top:0;left:0;
  z-index:99999;	
}
header .w3-row{
	margin:0;
}
header .logo {
    float: left;
    margin-top: 0;
}
header .cta_sec {
    float: right;
    margin-top: 2px;
}

header.fixed {
    background-color: #861511;
    box-shadow: 0 0 6px rgba(0,0,0,.4);
    padding-bottom: 20px;
    position: fixed;
    top: 0;
    z-index: 99999;
}

header.fixed .logo img{
    width: 200px;
    transition: transform 2s;
}
.logo img{
    transition: transform 2s;
}
.cta_top{
  text-align:right;	
}
.cta_top a {
    display: inline-block;
    margin-right: 6px;
    font-size: 14px;
}

.headerBook{
	border: 2px solid #fff;
    border-radius: 30px;
    background-color: white;
    padding: 0px 10px;
    text-decoration: none;
    transition: 2s ease;
}

.headerBook:hover{
    background-color: transparent;
    color: #fff;
    transition: 2s ease;
}

.headerBook:hover i{
    color:#fff;
    transition: 2s ease;
}

.cta_top a i {
   line-height: 28px;
   color: #e0221a;
   transition: 2s ease;
}

.cta_bottom a{
  display:inline-block;
  padding:5px 15px;
  background-color:#FFF;
  border-radius:10px;
  color:#000;
  margin-right:10px;
  text-decoration:none;	
  font-size:20px;
  line-height:22px;
  font-family:'Josefin Sans', sans-serif;
  
}
.cta_top a:last-child {
    margin-right: 0px;
}
.cta_bottom a:last-child{
	margin-right:0;
}
.cta_bottom a:first-child span{
    font-size:26px;
    color:#fff;
}
.cta_bottom a:last-child span{
    display:inline-block;
    width:25px;
    height:25px;
    border-radius:50%;
    background-color:#eca826;
    color:#FFF;
    font-size:18px;
    line-height:30px;
    text-align:center;
    margin-right:7px;
}

.owl-dots{
  width:100%;
  text-align:center;	
  margin-top:20px;
}
.owl-dots .owl-dot{
  display:inline-block;
  width:14px;
  height:14px;
  border-radius:50%;
  background-color:#209b85;
  margin-right:15px;	
}
.owl-dots .owl-dot.active{
  background-color:#fb9d4c;	
}
.owl-dots .owl-dot:last-child{
  margin-right:0;	
}

.whatsapp{
    transition: transform 2s;
}

.whatsapp:hover img, .whatsapp:focus .img, .whatsapp:active .img {
    transform: rotatex(360deg);
    transition: transform 2s;
}


/*--  header end here  --*/

/*---  home_banner start here  ---*/

.home_banner{
  padding:0;
  position:relative;
  max-width:1600px;
  margin:0 auto;
}
.home_banner img{
  width:100%;	
}
.home_banner_inner{
  width:100%;
  max-width:1400px;
  position:absolute;
  left:0;right:0;
  margin:auto;
  top:0;bottom:0;
      z-index: 9;
}
.home_banner .intro{
  width:100%;
  max-width:680px;
  position:absolute;
  left:40px;
  top:50%;
  -webkit-transform:translateY(-50%);
  transform:translateY(-50%);
}
.home_banner .intro h1{
  font-size:45px;
  line-height:55px;
  color:#FFF;
  text-transform:uppercase;	
  font-weight:700;
}
.home_banner .intro h1 span{
	display:block;
	text-transform:none;
	font-size:32px;
	line-height:40px;
	color:#eca826;
	font-weight:400;
}




/*-----  enquiry_ form  start here -----*/
.enquiry_form{
  width:100%;
  max-width:322px;
  padding:25px;
  background-color:#FFF;
  border-radius:20px;	
  position:absolute;
  right:40px;
  top:50%;
  -webkit-transform:translateY(-50%);
  transform:translateY(-50%);
}
.enquiry_form h3 {
    margin-top: 0;
    text-align: center;
    font-size: 15px;
    font-weight: 700;
    line-height: 24px;
    color: #000;
    /* text-transform: uppercase; */
    margin-bottom: 20px;
    letter-spacing: 0.7px;
}
.enquiry_form h3 span{
	color: #e0221a;
}
.input_box{
  width:100%;
  position:relative;
  height:35px;
  margin-bottom:15px;	
}
.input_box.msg{
	height:85px;
}
.input_box input,
.input_box textarea{
  display:block;
  width:100%;
  height:100%;
  background-color:#f0f0f0;
  border:1px solid #cacaca;	
  border-radius:5px;
  font-size:12px;
  padding-left:50px;
}
.input_box textarea{
	padding-top:7px;
}
.input_box span{
  display:block;
  width:42px;
  height:20px;
  border-right:1px solid #FFF;
  text-align:center;
  position:absolute;
  top:7px;
  left:0;
  font-size:20px;
  line-height:20px;
  color:#707070;
}

.enquiry_form .submit_btn{
   width:100%;
   text-align:center;	
}
.enquiry_form .submit_btn button {
    display: inline-block;
    padding: 10px 15px;
    background-color: #871511;
    color: #FFF;
    font-size: 14px;
    line-height: 18px;
    border: 0;
    font-weight: normal;
    border-radius: 29px;
    text-transform: uppercase;
    border: 2px solid #871511;
    transition: 2s ease;
}

.enquiry_form .submit_btn button:hover {
    border: 2px solid #871511;
    background-color: #fff;
    color: #871511;
     transition: 2s ease;
}



/*---  home_banner end here  ---*/

/*---  why_us start here  ---*/
.why_us{
  max-width:1500px;
  margin:0 auto;
  margin-top:-50px;
  position:relative;
  z-index:9;
  padding-bottom:100px;
  background-image:linear-gradient(to bottom, rgba(0,0,0,0), rgba(0,0,0,0), rgba(0,0,0,0) ,rgba(147,223,252,.2) );	
}
.usp_box{
  width:100%;
  text-align:center;
  padding:10px 0;	
}
.usp_box div{
	width:100%;
}
.usp_box .icon_box{
   margin-bottom:20px;	
   text-align:center;
}
.usp_box .icon_box img{
    display:inline-block;
    width:100%;
    max-width:113px;
    border-radius:50%;
}
.usp_box .icon_box:hover img{
    position:relative;
    top:-3px;
    border:3px solid #ec8226;
}
.usp_box .info{
    width:100%;
}
.usp_box h3{
  font-size:18px;
  line-height:22px;
  color:#ec8226;
  text-transform:uppercase;	
  font-weight:600;
}
.why_us:after{
  content:'';
  display:block;
  width:100%;
  max-width:570px;
  height:100%;
  background:url('../images/doctor_.png') no-repeat center top;
  position:absolute;
  right:0;
  top:-100px;
  z-index:99;	
}
.why_us:before{
	content:'';
	display:block;
	width:100%;
	height:145px;
	background-color:#FFF;
	position:absolute;
	right:0;
	bottom:-60px;
	transform:skewY(-5deg);
	z-index:101;
}


/*---  why_us end here  ---*/

/*----   usps sections start here  ----*/
.usps_sec {
    background-color: #871511;
    width: 90%;
    margin: 0 auto;
    margin-top: -21px;
    z-index: 1;
    display: block;
}

#counter_sec{
	position: relative;
	    margin-bottom: 96px;
}

.usps_box{
	text-align: center;
}

.usps_sec {
    background-color: #e0221a;
    width: 90%;
    margin: 0 auto;
    z-index: 1;
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    top: -76px;
    box-shadow: 3px 4px 9px 0px #de726e96;
    border-radius: 10px;
}
.usps_sec .lastUsp{
    border-right:0;	
}
.usps_sec .usps_box h2 {
    font-size: 39px;
    line-height: 8px;
    color: #FFF;
    font-weight: 600;
    margin-top: 20px;
}
.usps_sec .usps_box p{
   font-size:16px;
   line-height:18px;
   color:#FFF;	
}


/*----   usps sections end here  ----*/

/*----   treatment sections start here  ----*/

.heading h1{
	text-align: center;
    text-transform: uppercase;
    margin-top: 35px;
    font-size: 41px;
	margin-bottom: 0;
}

.headingHr{
	width: 65px;
    border-top: 2px solid #871511;
    border-top-style: dotted;
    margin: 0 auto;
	transition: 1s;
}

.heading p {
    text-align: center;
    font-size: 18px;
    color: #565656;
    margin-top: 7px;
}

.heading:hover > .headingHr {
    width: 155px;
    transition: 1s;
}

.treatment_box img {
    width: auto !important;
    margin: 0 auto;
}

.treatment_box {
    border: 2px solid #871511;
    margin-right: 40px;
    text-align: center;
    
    padding: 20px;
    border-radius: 32px;
    margin-bottom: 12px;
}
.treatment_box:hover p{
    color: #e0221a;
}

.treatment_box:hover{
    border: 2px solid #e0221a;
    box-shadow: 5px 6px 8px 0px rgba(0, 0, 0, 0.15);
}

.treatment_box p{
    font-size: 19px;
    margin-bottom: 0px;
}
.treatmentSlider{
	padding: 20px 110px;
}

#treatment_carousel .owl-prev {
    position: absolute;
    left: -52px;
    top: 76px;
    background-color: #871511;
    color: white;
    padding: 6px 10px;
    border-radius: 5px;
    border: 1px solid #871511;
    transition: 1s ease;
}

#treatment_carousel .owl-next{
	position: absolute;
    right: -13px;
    top: 76px;
    background-color: #871511;
    color: white;
    padding: 6px 10px;
    border-radius: 5px;
    border: 1px solid #871511;
    transition: 1s ease;
}

#treatment_carousel .owl-prev:hover, #treatment_carousel .owl-next:hover {
    background-color: transparent;
    color: #871511;
    transition: 1s ease;
}

..treatment_box:last-child{
    margin-right: 0px;
}


/*----   treatment sections end here  ----*/


