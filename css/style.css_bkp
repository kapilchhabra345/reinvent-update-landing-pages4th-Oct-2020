/*=======  Responsive   ======*/


@media (min-width:768px) and (max-width:1024px){
    .whyChooseLeft ul{
        padding: 0;
    }
    .input_box.msg {
        height: 51px;
    }
    .logo img {
        transition: transform 2s;
        width: 200px;
    }
    #counter_sec {
        position: relative;
        margin-bottom: 0;
    }
    .usps_sec {
        background-color: #e0221a;
        width: 100%;
        margin: 0 auto;
        z-index: 1;
        display: block;
        position: relative;
        left: 0;
        right: 0;
